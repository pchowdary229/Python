   "source": [
    "def is_palindrome(string):\n",
    "    stack = list(string)\n",
    "    reversed_string = ''.join(stack[::-1])\n",
    "    return string == reversed_string\n",
    "\n",
    "print(is_palindrome(\"madam\"))  # True\n",
    "print(is_palindrome(\"madan\"))  # False\n"
   ]
  }
